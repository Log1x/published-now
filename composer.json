{
  "name": "log1x/published-now",
  "type": "wordpress-muplugin",
  "license": "MIT",
  "description": "Quickly change a post publish date to the current date and time.",
  "keywords": [
    "wordpress"
  ],
  "require": {
    "php": ">=7.1.3"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5"
  },
  "scripts": {
    "lint": [
      "phpcs --ignore=vendor --extensions=php --standard=PSR12 ."
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
